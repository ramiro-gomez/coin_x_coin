{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-tabs": "off",
        "indent": ["error", "tab", { "SwitchCase": 1 }],
        "no-console": "off",
        "lines-between-class-members": "off",
        "import/no-mutable-exports": "off",
        "no-param-reassign": "off"
    }
}
